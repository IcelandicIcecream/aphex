{
	"name": "@aphexcms/resend-adapter",
	"version": "0.1.1",
	"type": "module",
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts",
			"default": "./src/index.ts"
		},
		"./schema": {
			"types": "./src/schema.ts",
			"import": "./src/schema.ts",
			"default": "./src/schema.ts"
		}
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"main": "./dist/index.js",
		"types": "./dist/index.d.ts",
		"exports": {
			".": {
				"types": "./dist/index.d.ts",
				"import": "./dist/index.js",
				"default": "./dist/index.js"
			},
			"./schema": {
				"types": "./dist/schema.d.ts",
				"import": "./dist/schema.js",
				"default": "./dist/schema.js"
			}
		}
	},
	"scripts": {
		"build": "tsc",
		"prepublishOnly": "pnpm build"
	},
	"dependencies": {
		"@aphexcms/cms-core": "workspace:*",
		"resend": "^4.0.1"
	},
	"peerDependencies": {
		"@aphexcms/cms-core": "workspace:*"
	},
	"devDependencies": {
		"@aphexcms/cms-core": "workspace:*",
		"tsup": "^8.0.0",
		"typescript": "^5.3.3"
	}
}
