{
    "name": "@aphexcms/playground",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev --force",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"db:start": "docker compose up -d",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"generate:types": "aphex generate:types ./src/lib/schemaTypes/index.ts ./src/lib/generated-types.ts"
	},
	"dependencies": {
		"@aphexcms/cms-core": "^0.2.3",
		"@aphexcms/graphql-plugin": "^0.1.6",
		"@aphexcms/postgresql-adapter": "^0.1.7",
		"@aphexcms/resend-adapter": "^0.1.2",
		"@aphexcms/storage-s3": "^0.1.1",
		"@aphexcms/ui": "^0.1.8",
		"better-auth": "^1.3.24",
		"drizzle-orm": "^0.40.0",
		"lucide-svelte": "^0.544.0",
		"mode-watcher": "^1.1.0",
		"postgres": "^3.4.5",
		"sharp": "^0.34.4"
	},
	"devDependencies": {
		"@internationalized/date": "^3.9.0",
		"@lucide/svelte": "^0.544.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"@types/node": "^22.0.0",
		"bits-ui": "^2.11.1",
		"clsx": "^2.1.1",
		"dotenv": "^16.4.5",
		"drizzle-kit": "^0.30.2",
		"graphql": "^16.11.0",
		"graphql-yoga": "^5.16.0",
		"mdsvex": "^0.12.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.0.0",
		"tsx": "^4.19.2",
		"tw-animate-css": "^1.3.8",
		"typescript": "^5.0.0",
		"vite": "^7.0.4"
	}
